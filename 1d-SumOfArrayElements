#include <iostream>
using namespace std;

int main()
{
    int Marks[5];
    int sum=0;
    int length=sizeof(Marks)/sizeof(Marks[0]);
    cout<<"Enter Array Elements"<<endl;

    for(int i=0;i<length;i++)
    {
        cout<<"Enter Array Element:-"<<(i+1)<<endl;
        cin>>Marks[i];

    }
    for(int i=0;i<length;i++)
    {
        sum=sum+Marks[i];
    }
cout<<"Array Elements are:"<<endl;

    for(int i=0;i<length;i++)
    {
        cout<<Marks[i]<<endl;
    }
    cout<<"Sum of Array Elements are: "<<sum;
    return 0;
}
